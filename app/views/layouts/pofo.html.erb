    <!-- Elimina paragrapho -->
    <script>
      $(document).on "turbolinks:load", ->
        $('form').on 'click', '.remove_fields', (event) ->
          $(this).prev('input[type=hidden]').val('1')
          $(this).closest('fieldset').hide()
          event.preventDefault()
    </script>
